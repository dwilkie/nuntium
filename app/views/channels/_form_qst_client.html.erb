<%= form_for_channel channel do |f| -%>
  <%= render 'edit_header', :f => f -%>
  <p>
  <%= f.label :url -%><br/>
  <%= f.text_field :url -%>
  </p>
  <p>
  <%= f.label :user -%><br/>
  <%= f.text_field :user -%>
  </p>
  <p>
  <%= f.label :password -%><br/>
  <%= f.password_field :password -%>
  </p>
  <%= render 'edit_footer', :f => f %>
<%- end -%>
