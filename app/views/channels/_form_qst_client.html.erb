<%= form_for_channel channel do |f| -%>
  <%= render 'edit_header', :f => f -%>
  <h5>QST client account info</h5>
  <p>
  <%= f.label :url -%><br/>
  <%= f.text_field :url -%>
  </p>
  <p>
  <%= f.label :user -%><br/>
  <%= f.text_field :user, :autocomplete => :off -%>
  </p>
  <p>
  <%= f.label :password -%><br/>
  <%= f.password_field :password, :autocomplete => :off -%>
  </p>
  <%= render 'edit_footer', :f => f %>
<%- end -%>
